{
  "name": "@pnpm/node.path",
  "version": "1.0.0",
  "description": "Resolves directory locations and bin specifiers of managed Node.js versions",
  "funding": "https://opencollective.com/pnpm",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "!*.map"
  ],
  "exports": {
    ".": "./lib/index.js"
  },
  "engines": {
    "node": ">=18.12"
  },
  "scripts": {
    "lint": "eslint \"src/**/*.ts\" \"test/**/*.ts\"",
    "_test": "jest",
    "test": "pnpm run compile && pnpm run _test",
    "prepublishOnly": "pnpm run compile",
    "compile": "tsc --build && pnpm run lint --fix"
  },
  "repository": "https://github.com/pnpm/pnpm/blob/main/env/node.path",
  "keywords": [
    "pnpm9",
    "pnpm",
    "env",
    "node.js"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pnpm/pnpm/issues"
  },
  "homepage": "https://github.com/pnpm/pnpm/blob/main/env/node.path#readme",
  "peerDependencies": {
    "@pnpm/logger": "^5.0.0"
  },
  "dependencies": {
    "@pnpm/config": "workspace:*",
    "@pnpm/error": "workspace:*",
    "@pnpm/fetch": "workspace:*",
    "@pnpm/node.fetcher": "workspace:*",
    "@pnpm/node.mirror": "workspace:*",
    "@pnpm/node.specifier": "workspace:*",
    "@pnpm/store-path": "workspace:*",
    "load-json-file": "catalog:",
    "write-json-file": "catalog:"
  },
  "devDependencies": {
    "@pnpm/node.path": "workspace:*"
  }
}
