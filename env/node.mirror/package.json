  "bugs": {
    "url": "https://github.com/pnpm/pnpm/issues"
  },
  "homepage": "https://github.com/pnpm/pnpm/blob/main/env/node.mirror#readme",
  "dependencies": {
    "@pnpm/config": "workspace:*"
  },
  "devDependencies": {
    "@pnpm/node.mirror": "workspace:*"
  }
